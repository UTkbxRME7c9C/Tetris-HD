public class Block {
 color c;
 public Block(color c_) {
   c = c_;
   fill(c);
   rect (x,y,10,10);
 }
 
 void setColor(color newColor){
   c = newColor;
 }
 
 // what is this supposed to do? do we implement this into grid functions?
 void remove (Block b) {
   if (b != null) {
     noStroke();
     fill(255);
     rect(b.x, b.y,10,10);
   }
 }
}
